{
  "name": "eslint-formatter-proxy",
  "version": "0.1.1",
  "description": "",
  "author": "Jannis Morgenstern <jannis.n.morgenstern@gmail.com> (https://github.com/nyarthan)",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-formatter",
    "eslintformatter"
  ],
  "homepage": "https://github.com/nyarthan/eslint-formatter-proxy?tab=readme-ov-file",
  "repository": "github:nyarthan/eslint-formatter-proxy",
  "bugs": "https://github.com/nyarthan/eslint-formatter-proxy/issues",
  "packageManager": "pnpm@10.18.0",
  "type": "module",
  "imports": {
    "#package.json": "./package.json"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "build": "rslib build",
    "release": "pnpm publish --access public --provenance",
    "prerelease": "pnpm build",
    "knip": "knip --cache --include dependencies,files,exports,classMembers,nsTypes,nsExports,duplicates,binaries,types"
  },
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@commitlint/format": "20.0.0",
    "@commitlint/types": "20.0.0",
    "@eslint/js": "9.37.0",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@nyarthan/prettier-config": "1.0.3",
    "@rslib/core": "0.15.0",
    "@types/node": "24.6.2",
    "conventional-changelog-atom": "5.0.0",
    "eslint": "9.37.0",
    "globals": "16.4.0",
    "knip": "5.64.1",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0"
  },
  "peerDependencies": {
    "eslint": ">=9"
  }
}
